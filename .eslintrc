{
    "extends": "airbnb",
    "rules": {
        "indent": [2, 4],
        "react/jsx-indent-props": [2, 4],
        "react/jsx-indent": ["error", 4],
        "import/no-unresolved": 0,
        "no-unused-expressions": [2, { allowShortCircuit: true }],
        "no-script-url": 0,
    },
    "globals": {
        "$": true,
        "template": true,
    }
}
